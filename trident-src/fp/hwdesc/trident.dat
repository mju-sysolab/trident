    (port out 
      (name vec_dp) 
      (type std_logic) 
      (size 64))
    (map vec_fp in0)
    (map clk clk)
    (map vec_dp out)
  )
  
  (libobject dp_to_fp tr_dptofp
    (port in 
      (name vec_dp) 
      (type std_logic) 
      (size 64))
    (port in 
      (name clk) 
      (type std_logic) 
      (size 1))
    (port out 
      (name vec_fp) 
      (type std_logic) 
      (size 32))
    (map vec_dp in0)
    (map clk clk)
    (map vec_fp out)
  )
  
  (libobject fp_to_i tr_fptoi
